{
  "name": "directory-module-webpack-plugin",
  "version": "0.1.0",
  "description": "Webpack plugin generating modules based on directory contents",
  "keywords": [
    "webpack", "plugin", "directory"
  ],
  "main": "plugin.js",
  "scripts": {
    "build": "tsc -p src",
    "test": "cross-env TS_NODE_PROJECT=test nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm test && npm run build",
    "lint": "tslint src/*.ts test/*.ts"
  },
  "author": "Gyusun Yeom <omniavinco@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird-global": "^3.5.5",
    "@types/chai": "^4.1.4",
    "@types/extract-text-webpack-plugin": "^3.0.3",
    "@types/lodash.assign": "^4.2.3",
    "@types/lodash.frompairs": "^4.0.3",
    "@types/lodash.partial": "^4.2.3",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.6.22",
    "@types/webpack": "^3.8.13",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "raw-loader": "^0.5.1",
    "source-map-support": "^0.5.6",
    "ts-loader": "^3.5.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "webpack": "^3.12.0"
  },
  "peerDependencies": {
    "webpack": "^3.11.0 || ^4.0.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "bluebird-global": "^1.0.1",
    "json-beautify": "^1.0.1",
    "lodash.assign": "^4.2.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.partial": "^4.2.1",
    "minimatch": "^3.0.4"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "files": [
    "*.d.ts",
    "*.js",
    "emitter/*.js",
    "emitter/*.d.ts"
  ]
}
